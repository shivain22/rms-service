        <addForeignKeyConstraint baseColumnNames="supervisor_id"
                                 baseTableName="table_assignment"
                                 constraintName="fk_table_assignment__supervisor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="rms_user"
                                 />
    </changeSet>
</databaseChangeLog>
